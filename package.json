{
  "name": "whats-for-dinner-client",
  "version": "1.0.0",
  "description": "Front-end for Blue Ocean Blossom Team",
  "main": "index.js",
  "scripts": {
    "start": "forever start server/index.js",
    "test": "jest",
    "test-watch": "jest --watchAll --verbose",
    "test-cov": "jest --verbose --coverage",
    "react-dev": "webpack --config ./webpack.config.js --mode development -w",
    "server-dev": "npx nodemon server/index.js",
    "react-prod": "webpack --config ./webpack.config.js --mode production",
    "server-prod": "node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blue-Ocean-Team-Blossom/Whats-For-Dinner-Client.git"
  },
  "keywords": [
    "Grocery",
    "list",
    "recipes"
  ],
  "author": "Team Blossom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Blue-Ocean-Team-Blossom/Whats-For-Dinner-Client/issues"
  },
  "homepage": "https://github.com/Blue-Ocean-Team-Blossom/Whats-For-Dinner-Client#readme",
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.14.0",
    "axios": "^0.21.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "forever": "^3.0.4",
    "html-react-parser": "^1.2.6",
    "jquery": "^3.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.2.1",
    "jest-enzyme": "^7.1.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    }
  }
}
